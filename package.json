{
  "name": "family-graph",
  "version": "1.2.0",
  "private": true,
  "description": "",
  "main": "bin/www",
  "dependencies": {
    "body-parser": "^1.14.2",
    "cas-authentication": "0.0.8",
    "cookie-parser": "^1.4.1",
    "debug": "^2.2.0",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "morgan": "^1.6.1",
    "mysql": "^2.11.1",
    "mysql2": "^1.5.3",
    "nyc": "^12.0.2",
    "sequelize": "^4.38.0",
    "serve-favicon": "^2.3.0",
    "socket.io": "^2.1.1",
    "twig": "^0.8.7"
  },
  "devDependencies": {
    "babel": "6.3.13",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.2",
    "babel-preset-env": "^1.7.0",
    "chai": "^3.5.0",
    "chai-http": "^4.0.0",
    "coffeescript": "^2.3.1",
    "coveralls": "^3.0.2",
    "dictum.js": "^1.0.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.19.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-config-prettier": "^2.3.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-prettier": "^2.1.1",
    "husky": "^0.13.2",
    "istanbul": "^0.4.3",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.2.0",
    "nodemon": "^1.17.2",
    "nsp": "^3.2.1",
    "prettier": "^1.11.1",
    "prettier-eslint": "^8.2.1",
    "prompt": "^1.0.0",
    "sequelize-cli": "^4.0.0"
  },
  "scripts": {
    "start": "node ./bin/www",
    "cover": "NODE_ENV=testing istanbul cover ./node_modules/mocha/bin/_mocha -- --require babel-core/register test/app.js",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "nsp": "./node_modules/.bin/nsp check",
    "lint": "./node_modules/eslint/bin/eslint.js \"**/*.js\"",
    "lint-diff": "git diff --name-only --cached --relative | grep \\\\.js$ | xargs ./node_modules/eslint/bin/eslint.js",
    "lint-fix": "./node_modules/eslint/bin/eslint.js \"**/*.js\" --fix",
    "precommit": "npm run lint-diff",
    "outdated": "npm outdated --depth 0",
    "pretest": "",
    "prestart": "npm run lint",
    "test": "NODE_ENV=testing ./node_modules/mocha/bin/mocha --require babel-core/register test/app.js && npm run nsp",
    "create-migration": "./node_modules/.bin/sequelize migration:create --name='my-migration-name' --config ./migrations/config.js --migrations-path ./migrations/migrations",
    "migrations": "./node_modules/.bin/sequelize --config ./migrations/config.js --migrations-path ./migrations/migrations db:migrate"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "author": "Cesar Richard <cesar.richard@u-psud.fr>",
  "license": "CC-BY-NC-ND-4.0",
  "homepage": "https://github.com/cesar-richard/family-graph",
  "repository": {
    "type": "git",
    "url": "https://github.com/cesar-richard/family-graph.git"
  },
  "bugs": {
    "url": "https://github.com/cesar-richard/family-graph/issues",
    "email": "cesar.richard@u-psud.fr"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules/**/node_modules"
    ],
    "verbose": true,
    "execMap": {
      "js": "node --harmony"
    },
    "watch": [
      "**.js",
      "**.json",
      "**.twig",
      "**.css"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js,json,twig"
  }
}
