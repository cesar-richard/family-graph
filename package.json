  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules/**/node_modules"
    ],
    "verbose": true,
    "execMap": {
      "js": "node --harmony"
    },
    "watch": ["**.js","**.json","**.twig","**.css"],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js,json,twig"
